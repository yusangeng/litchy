{
  "name": "litchy",
  "version": "2.0.4",
  "description": "EcmaScript infrastructure.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "node_modules/.bin/babel-node node_modules/.bin/isparta cover node_modules/.bin/_mocha",
    "build-debug": "node_modules/.bin/webpack --color --config ./webpack/debug.js",
    "build-release": "node_modules/.bin/webpack --color --config ./webpack/release.js",
    "dep-analyze": "node_modules/.bin/webpack --color --config ./webpack/debug.js --profile --json > ./.package/DependenceNetwork.json",
    "prebuild": "npm test",
    "build": "rm -rf ./.package && npm run build-debug && npm run build-release",
    "es5": "rm -rf ./lib && ./node_modules/.bin/babel ./src -d ./lib --source-maps inline",
    "prepublish": "npm test && npm run es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/litchy.git"
  },
  "keywords": [
    "infra",
    "mixin",
    "decorator"
  ],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.0",
    "isparta": "^4.0.0",
    "istanbul": "0.4.4",
    "mocha": "^3.5.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "dependencies": {
    "fast-deep-equal": "^1.0.0",
    "mix-with": "^1.0.2"
  }
}
